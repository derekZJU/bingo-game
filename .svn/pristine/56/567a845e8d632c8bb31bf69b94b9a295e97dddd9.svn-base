package dw35_wz23.server.model;

import java.io.Serializable;

/**
 * Install Message implements Serializable to install the game
 * @author dw35, wz23
 *
 */
public class InstallMsg implements Serializable{
	private static final long serialVersionUID = -6637044128757080515L;
	int teamID;
	
	/**
	 * install message
	 * @param teamID
	 *              the index number of team
	 */
	public InstallMsg(int teamID){
		this.teamID = teamID;
	}
	/**
	 * get the team id
	 * @return team id
	 */
	public int getTeamID() {
		return teamID;
	}
	/**
	 * set the team id 
	 * @param teamID team id
	 */
	public void setTeamID(int teamID) {
		this.teamID = teamID;
	}
	
}
